<group name="samba-auth">
  <rule id="100000" level="5">
    <decoded_as>samba-json-auth</decoded_as>
    <field name="samba_event.category">authentication</field>
    <field name="samba_event.status">failure</field>
    <description>Samba authentication failed</description>
    <group>samba,auth,failed_login</group>
    <mitre>
      <id>T1110</id>
    </mitre>
  </rule>
  <rule id="100001" level="2">
    <decoded_as>samba-json-auth</decoded_as>
    <field name="samba_event.category">authentication</field>
    <field name="samba_event.status">success</field>
    <description>Samba authentication successful</description>
    <group>samba,auth</group>
  </rule>
</group>
